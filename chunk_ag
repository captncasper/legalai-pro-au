app = FastAPI(title="Legal AI API - Working Version")

app.add_middleware(
    CORSMiddleware,
    allow_origins=["*"],
    allow_methods=["*"],
    allow_headers=["*"],
)

# Request Models
class QuantumRequest(BaseModel):
    case_type: str
